{
	"name": "@tint-ui/tooltip",
	"version": "0.0.1",
	"author": "phragon@websoftlab.com",
	"license": "MIT",
	"scripts": {
		"build": "phragon-script build --cwd \"../../\" -P @tint-ui/tooltip",
		"postbuild": "node ../../scripts/main.js tooltip",
		"prettier": "phragon-script prettier --cwd \"../../\" -P @tint-ui/tooltip"
	},
	"dependencies": {
		"clsx": "^2.1.1",
		"@tint-ui/tools": "*",
		"@tint-ui/theme": "*",
		"@radix-ui/react-tooltip": "^1.2.7"
	},
	"devDependencies": {
		"@types/react": "^18.0.12"
	},
	"peerDependencies": {
		"react": "^16.8.0 || ^17 || ^18 || ^19"
	},
	"exports": {
		".": {
			"default": "./build/index.js"
		},
		"./package.json": {
			"default": "./package.json"
		},
		"./styles.css": {
			"require": "./build/styles.css",
			"default": "./build/styles.css"
		},
		"./styles.module.scss": {
			"require": "./build/styles.module.scss",
			"default": "./build/styles.module.scss"
		},
		"./styles.module.css": {
			"require": "./build/styles.module.css",
			"default": "./build/styles.module.css"
		}
	},
	"types": "build/index.d.ts",
	"typesVersions": {
		"*": {
			"build/index.d.ts": ["src/index.ts"],
			"*": ["src/*"]
		}
	}
}
