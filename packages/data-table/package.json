{
	"name": "@tint-ui/data-table",
	"version": "0.0.1",
	"author": "phragon@websoftlab.com",
	"license": "MIT",
	"scripts": {
		"build": "phragon-script build --cwd \"../../\" -P @tint-ui/data-table",
		"postbuild": "node ../../scripts/main.js data-table",
		"prettier": "phragon-script prettier --cwd \"../../\" -P @tint-ui/data-table"
	},
	"dependencies": {
		"clsx": "^2.1.1",
		"@tint-ui/tools": "*",
		"@tint-ui/theme": "*",
		"@tint-ui/button": "*",
		"@tint-ui/slide-navigator": "*",
		"@tint-ui/table": "*",
		"@tint-ui/input-checkbox": "*",
		"@tint-ui/input": "*",
		"@tint-ui/svg-icon": "*"
	},
	"devDependencies": {
		"@types/react": "^18.0.12",
		"@tanstack/react-table": "^8.21.3"
	},
	"peerDependencies": {
		"react": "^16.8.0 || ^17 || ^18 || ^19",
		"@tanstack/react-table": "^8.21.3"
	},
	"exports": {
		".": {
			"require": "./build/cjs/index.js",
			"default": "./build/index.js"
		},
		"./styles.json": {
			"require": "./styles.json",
			"default": "./styles.json"
		},
		"./package.json": {
			"require": "./package.json",
			"default": "./package.json"
		},
		"./styles.module.scss": {
			"require": "./build/styles.module.scss",
			"default": "./build/styles.module.scss"
		},
		"./styles.module.css": {
			"require": "./build/styles.module.css",
			"default": "./build/styles.module.css"
		},
		"./styles-filter.module.scss": {
			"require": "./build/styles-filter.module.scss",
			"default": "./build/styles-filter.module.scss"
		},
		"./styles-filter.module.css": {
			"require": "./build/styles-filter.module.css",
			"default": "./build/styles-filter.module.css"
		},
		"./styles-toolbar.module.scss": {
			"require": "./build/styles-toolbar.module.scss",
			"default": "./build/styles-toolbar.module.scss"
		},
		"./styles-toolbar.module.css": {
			"require": "./build/styles-toolbar.module.css",
			"default": "./build/styles-toolbar.module.css"
		},
		"./styles-pagination.module.scss": {
			"require": "./build/styles-pagination.module.scss",
			"default": "./build/styles-pagination.module.scss"
		},
		"./styles-pagination.module.css": {
			"require": "./build/styles-pagination.module.css",
			"default": "./build/styles-pagination.module.css"
		}
	},
	"types": "build/index.d.ts",
	"typesVersions": {
		"*": {
			"build/index.d.ts": ["src/index.ts"],
			"*": ["src/*"]
		}
	}
}
