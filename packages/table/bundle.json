{
	"src": [
		{
			"target": "node",
			"output": "./cjs"
		},
		{
			"target": "module",
			"output": "."
		},
		{
			"target": "types",
			"output": ".",
			"package.json": {
				"exports": {
					".": {
						"require": "./cjs/index.js",
						"default": "./index.js"
					},
					"./styles.module.scss": {
						"require": "./styles.module.scss",
						"default": "./styles.module.scss"
					},
					"./styles.module.css": {
						"require": "./styles.module.css",
						"default": "./styles.module.css"
					},
					"./styles.css": {
						"require": "./styles.css",
						"default": "./styles.css"
					},
					"./package.json": {
						"require": "./package.json",
						"default": "./package.json"
					}
				}
			}
		}
	]
}
